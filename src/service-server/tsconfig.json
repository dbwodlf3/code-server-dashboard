{
    "compilerOptions": {
        "resolveJsonModule": true,
        "target": "ES5",
        "module": "CommonJS",
        "inlineSourceMap": true,
        "rootDir": "./",
        "removeComments": true,
        "strict": true,
        "baseUrl": "./src",
        "outDir": "./out",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "charset": "utf8"
    },
    "include":["./src/**/*.ts"],
    "exclude": [
    ]
}